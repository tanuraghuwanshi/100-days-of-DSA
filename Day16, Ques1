// Given an array of integers, count the frequency of each distinct element and print the result.

#include <stdio.h>
int main() {
    int arr[] = {1, 2, 2, 3, 1, 4, 2};
    int n = sizeof(arr) 
    int visited[n];  
    for (int i = 0; i < n; i++) {
        visited[i] = 0;
    }
    for (int i = 0; i < n; i++) {
        if (visited[i] == 1)
            continue;
        int count = 1;
        for (int j = i + 1; j < n; j++) {
            if (arr[i] == arr[j]) {
                count++;
                visited[j] = 1;  
            }
        }
        printf("%d occurs %d times\n", arr[i], count);
    }
    return 0;
}
