// Read a string and check if it is a palindrome using two-pointer comparison.

#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    int i = 0, j;
    scanf("%s", s);
    j = strlen(s) - 1;
    while (i < j) {
        if (s[i] != s[j]) {
            printf("Not Palindrome");
            return 0;
        }
        i++;
        j--;
    }
    printf("Palindrome");
    return 0;
}
